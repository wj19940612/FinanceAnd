            android:screenOrientation="portrait"/>

        <!-- 交易 H5 -->
        <activity
            android:name=".activity.WebActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.trade.TradeWebActivity"
            android:screenOrientation="portrait"/>
