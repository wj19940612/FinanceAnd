            android:name=".activity.trade.PublishOpinionActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.CreditApproveActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.CreditActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.economiccircle.ContentImgActivity"
            android:screenOrientation="portrait"/>
        <activity
