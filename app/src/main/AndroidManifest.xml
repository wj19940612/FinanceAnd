            android:name=".activity.home.BorrowMoneyActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.home.OptionalActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.home.SearchOptionalActivity"
