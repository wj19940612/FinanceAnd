            android:name=".activity.mutual.BorrowDetailsActivity"
            android:screenOrientation="portrait"/>
        <!-- 观点大神 -->
        <activity
            android:name=".activity.opinion.OpinionActivity"
            android:screenOrientation="portrait"/>
        <!-- 我的模块 -->
        <activity
            android:name=".activity.mine.ModifyUserInfoActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.ModifyUserNameActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.ClipHeadImageActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.NewsActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.UserDataActivity"
