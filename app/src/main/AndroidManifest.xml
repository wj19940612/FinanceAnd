        <activity
            android:name=".activity.mine.AttentionActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.FansActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".activity.mine.PublishActivity"
